{
  "name": "beetpx-example-input-tester",
  "version": "0.0.0",
  "private": true,
  "author": "Beetroot Paul",
  "license": "MIT",
  "scripts": {
    "start": "beetpx dev --open --htmlTitle \"[dev] Input Tester | BeetPx example\" --htmlIcon html_icon_dev.png",
    "build": "beetpx build --htmlTitle \"Input Tester | BeetPx example\" --htmlIcon html_icon_prod.png",
    "prod": "npm run build && beetpx preview",
    "zip": "npm run build && beetpx zip",
    "format": "prettier --ignore-path .gitignore --write src/",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch"
  },
  "devDependencies": {
    "@beetpx/beetpx": "file:../../",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "3.2.3",
    "typescript": "5.1.6"
  }
}
